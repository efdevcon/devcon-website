    "react-svg-loader": "^3.0.3",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.32.8",
    "selenium-webdriver": "^4.0.0-beta.2",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.2.3"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "node_modules"
    ]
  }
}
